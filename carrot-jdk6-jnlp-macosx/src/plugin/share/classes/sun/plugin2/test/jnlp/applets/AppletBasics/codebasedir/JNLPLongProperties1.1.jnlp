<?xml version="1.0" encoding="utf-8"?>
<jnlp 
      href="JNLPLongProperties1.1.jnlp">
  <information>
    <title>JREVersion1.1 Applet (launch long props 1.1) rel</title>
    <vendor>SUN Deployment</vendor>
    <description kind='tootltip'>A simple JNLP test</description>
    <description kind='one-line'>Applet</description>
    <homepage href='JREVersion1-jnlp-applet-rel-LC-check1.html' />
    <offline-allowed/>
  </information>
  <resources>
    <j2se version="1.4+" java-vm-args='"-DminimumJRE=1.4.2" -client -DQueryResult.WarningMessage="The query exceeds the allowed number of search terms.NL Results matching the first 40 terms have been returned.NL This may include more or less results than what the full query would have returned."'/>
    <property name="QueryResult.LimitationCount" value="40" />
    <jar href="JRERelaunch1.jar"/>
    <extension name="SimpleApplet" href="SimpleApplet-jnlp-applet-rel.jnlp"/>
  </resources>
  <applet-desc 
      name="JRERelaunch1"
      main-class="JRERelaunch1"
      width="666" 
      height="999">
  </applet-desc>
</jnlp>
